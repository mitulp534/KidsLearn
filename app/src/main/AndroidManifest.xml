<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.hp.com.kidslearn">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AbcdActivity">
            <intent-filter>
                <action android:name="com.example.hp.com.kidslearn.AbcdActivity" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".AnimalsActivity">
            <intent-filter>
                <action android:name="com.example.hp.com.kidslearn.AnimalsActivity" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".BirdsActivity">
            <intent-filter>
                <action android:name="com.example.hp.com.kidslearn.BirdsActivity" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".FruitsActivity">
            <intent-filter>
                <action android:name="com.example.hp.com.kidslearn.FruitsActivity" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".VegetablesActivity">
            <intent-filter>
                <action android:name="com.example.hp.com.kidslearn.VegetablesActivity" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".VehiclesActivity">
            <intent-filter>
                <action android:name="com.example.hp.com.kidslearn.VehiclesActivity" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
    </application>

</manifest>